                                            <growablerows>3</growablerows>
                                            <rows>1</rows>
                                            <growablecols>2</growablecols>
                                            <cols>6</cols>
                                            <vgap>6</vgap>
                                            <object class="sizeritem">
                                                <border>6</border>
                                                <object class="wxButton" name="wxID_ADD">
